{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./",
        "lib": [
            "es6",
            "es7"
        ]
    },
    "files": [
        "index.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}